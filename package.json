{
  "name": "amylase",
  "version": "0.0.15",
  "description": "react router creator",
  "main": "build",
  "scripts": {
    "prepublish": "npm run test",
    "build": "babel src --out-dir build",
    "clean": "rimraf build",
    "lint": "eslint --ext js,jsx src test",
    "test": "npm run clean && npm run build && npm run reactrouter:13 && npm run test:only && npm run reactrouter:3 && npm run test:only && npm run reactrouter:4 && npm run test:only",
    "test:only": "mocha --recursive test",
    "reactrouter:clean": "rimraf node_modules/react-router node_modules/react-router-dom",
    "reactrouter:13": "npm run reactrouter:clean && npm i react-router@0.13.6 --no-save",
    "reactrouter:3": "npm run reactrouter:clean && npm i react-router@3.0.5 --no-save",
    "reactrouter:4": "npm run reactrouter:clean && npm i react-router@4.2.0 react-router-dom@4.2.2 --no-save"
  },
  "keywords": [
    "react-router"
  ],
  "author": "TangZhen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangzhen/amylase.git"
  },
  "dependencies": {
    "path-to-regexp": "^2.0.0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "6.26.0",
    "chai": "^3.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.18.0",
    "jsdom": "^11.1.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0"
  }
}
